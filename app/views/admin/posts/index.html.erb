<div class="container">
  <div class="row p-0">
    <div class="col-lg-3">
      <%= render 'layouts/side' %>
    </div>
    <div class="col-lg-9">
      <div class="text-right p-2">
        <%= link_to admin_post_report_posts_path, class: "text-danger h5" do %>
          >> 報告された投稿
        <% end %>
      </div>
      <div class="border-top">
        <div>
          <%= render 'layouts/search_post' %>
        </div>
        <div>
          <%= render 'layouts/posts', posts: @posts  %>
        </div>
      </div>
    </div>
  </div>
</div>
